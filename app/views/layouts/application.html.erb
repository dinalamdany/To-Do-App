<!DOCTYPE html>
<html>
<head>
  <title>ToDo | <%= @title %></title>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render 'layouts/header' %>
  <section class="round">
	<% flash.each do |key, value| %>
	<div class="flash <%= key %>"><%= value %></div>
	<% end %>
	<%= yield %>
  </section>
<%= debug(params) if Rails.env.development? %>
</body>
</html>
